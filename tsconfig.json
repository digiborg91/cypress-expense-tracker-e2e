{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": ".", 
    "types": ["cypress"]
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.js"
  ],
  "exclude": ["node_modules", "dist"]
}
